{
  "name": "use-eazy-auth",
  "version": "0.0.1",
  "sideEffects": false,
  "description": "React hooks for handle auth stuff",
  "main": "./lib/index.cjs.js",
  "module": "./lib/index.esm.js",
  "repository": "https://github.com/gffuma/use-eazy-auth",
  "author": "Giovanni Fumagalli <fumagalli.gf@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gffuma/use-eazy-auth/issues"
  },
  "keywords": [
    "react",
    "hooks",
    "auth"
  ],
  "scripts": {
    "format": "prettier --no-semi --single-quote --trailing-comma es5  --write \"{src,__{tests,mocks}__}/**/*.js\"",
    "prebuild": "rimraf lib",
    "test": "jest",
    "build": "rollup -c",
    "build:watch": "rollup -c -w",
    "dev": "webpack-dev-server"
  },
  "eslintConfig": {
    "extends": "react-app",
    "plugins": [
      "react-hooks"
    ],
    "rules": {
      "react-hooks/rules-of-hooks": "error",
      "react-hooks/exhaustive-deps": "warn"
    }
  },
  "peerDependencies": {
    "react": "^16.8.0",
    "react-dom": "^16.8.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "9.x",
    "babel-jest": "^24.7.1",
    "babel-loader": "^8.0.5",
    "css-loader": "^2.1.1",
    "eslint": "5.12.0",
    "eslint-config-react-app": "^3.0.8",
    "eslint-plugin-flowtype": "2.x",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-react-hooks": "^1.6.0",
    "jest": "^24.7.1",
    "jest-dom": "^3.1.3",
    "prettier": "1.16.4",
    "react": "16.9.0-alpha.0",
    "react-dom": "16.9.0-alpha.0",
    "react-testing-library": "^6.1.2",
    "rimraf": "^2.6.3",
    "rollup": "^1.8.0",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.3.0",
    "rollup-plugin-node-resolve": "^4.0.1",
    "style-loader": "^0.23.1",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.2.1"
  }
}
